<div class="container">
  <h1>New article</h1>

  <div class="form-group">
    <%= form_for(@article) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :content %>
    <%= f.text_field :content, class: 'form-control' %>
  </div>

  <div class="input">
    <%= f.submit class: 'btn btn-primary' %>
  </div>

    <% end %>

  <div>
    <%= link_to "Back", articles_path %>
  </div>

</div>
